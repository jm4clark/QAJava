
public class User {
	private static int counter = 0;
	private int ID;
	private String name;
	private int age;

	public User(String name, int age) {
		this.name = name;
		this.age = age;
		counter++;
		ID = counter;
	}

	public int getID() {
		return ID;
	}

	public void setID(int iD) {
		this.ID = iD;
	}
}
