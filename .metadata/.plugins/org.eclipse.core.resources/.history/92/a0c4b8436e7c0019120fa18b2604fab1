
public class Vehicle {
	protected int wheels;
	protected String colour;
	protected int topSpeed;
	protected int ID;
	protected double price;

	public Vehicle(int wheels, String colour, int topSpeed, int ID, double price) {
		this.wheels = wheels;
		this.colour = colour;
		this.topSpeed = topSpeed;
		this.ID = ID;
		this.price = price;
	}

}
